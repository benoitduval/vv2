<div class="calendar-container">
  <div id="calendar"></div>

  <!-- Edit Dialog -->
  <div class="modal fade" id="editNewEvent" aria-hidden="true" aria-labelledby="editNewEvent"
    role="dialog" tabindex="-1" data-show="false">
    <div class="modal-dialog modal-simple modal-sidebar w-350">
      <div class="modal-content" style="height:100%;">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body">
          <h4 class="text-uppercase" id="modal-title"></h4>
          <div>
            <p class="mb-5 text-nowrap"><i class="icon wb-home mr-10" aria-hidden="true"></i>
              <span class="text-break" id="modal-place"></span>
            </p>
            <p class="mb-5 text-nowrap"><i class="icon wb-map mr-10" aria-hidden="true"></i>
              <span class="text-break" id="modal-address"></span>, 
              <span class="text-break" id="modal-zipcode"></span> 
              <span class="text-break" id="modal-city"></span>
            </p>
            <p class="mb-5 text-nowrap"><i class="icon wb-time mr-10" aria-hidden="true"></i>
              <span class="text-break" id="modal-date"></span>
            </p>
          </div>

          <div class="btn-group btn-group-justified mt-20 mb-40">
            <div class="btn-group" role="group">
              <a href="#" class="btn btn-outline btn-default" id="event-place-url" target="_blank">
                <i class="icon wb-map" aria-hidden="true"></i>
                <span class="text-uppercase">go to map</span>
              </a>
            </div>

            <div class="btn-group" role="group">
              <a href="#" class="btn btn-outline btn-info event-url">
                <i class="icon wb-eye" aria-hidden="true"></i>
                <span class="text-uppercase">See details</span>
              </a>
            </div>
          </div>

          <hr class="mt-20 mb-20">

          <h4 id="modal-title" class="text-uppercase text-center"><i class="icon wb-check-circle" aria-hidden="true"></i>Your Disponibility</h4>
          <div class="btn-group btn-group-justified mt-20 mb-40">
            <div class="btn-group" role="group">
              <a id="event-url-ok" href="#" class="btn btn-present">
                <i class="icon wb-check-circle" aria-hidden="true"></i>
                <br>
                <span class="text-uppercase">In</span>
              </a>
            </div>

            <div class="btn-group" role="group">
              <a id="event-url-no" href="#" class="btn btn-absent">
                <i class="icon wb-minus-circle" aria-hidden="true"></i>
                <br>
                <span class="text-uppercase">Out</span>
              </a>
            </div>

            <div class="btn-group mt-40" role="group">
              <a id="event-url-incertain" href="#" class="btn btn-uncertain">
                <i class="icon wb-help-circle" aria-hidden="true"></i>
                <br>
                <span class="text-uppercase">Uncertain</span>
              </a>
            </div>
          </div>

          <hr class="mt-20 mb-20">

          <h4 id="modal-title" class="text-uppercase text-center"><i class="icon wb-users" aria-hidden="true"></i>Your Team</h4>

          <div class="example-wrap mt-20 mb-0">
            <div class="nav-tabs-horizontal" data-plugin="tabs">
              <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item" role="presentation">
                  <a class="nav-link active show" data-toggle="tab" href="#exampleTabsIconOne" aria-controls="exampleTabsIconOne" role="tab" aria-selected="true">
                    <span>
                      <i class="icon wb-check-circle text-present" style="font-size:24px;" aria-hidden="true"></i>
                      <span class="badge badge-pill up badge-info" id="event-count-ok"></span>
                    </span>
                  </a>
                </li>
                <li class="nav-item" role="presentation">
                  <a class="nav-link" data-toggle="tab" href="#exampleTabsIconTwo" aria-controls="exampleTabsIconTwo" role="tab" aria-selected="false">
                    <span>
                      <i class="icon wb-minus-circle text-absent" style="font-size:24px;" aria-hidden="true"></i>
                      <span class="badge badge-pill up badge-info" id="event-count-no"></span>
                    </span>
                  </a>
                </li>
                <li class="nav-item" role="presentation">
                  <a class="nav-link" data-toggle="tab" href="#exampleTabsIconThree" aria-controls="exampleTabsIconThree" role="tab" aria-selected="false">
                    <span>
                      <i class="icon wb-help-circle text-uncertain" style="font-size:24px;" aria-hidden="true"></i>
                      <span class="badge badge-pill up badge-info" id="event-count-uncertain"></span>
                    </span>
                  </a>
                </li>
                <li class="nav-item" role="presentation">
                  <a class="nav-link" data-toggle="tab" href="#exampleTabsIconFour" aria-controls="exampleTabsIconFour" role="tab">
                    <span>
                      <i class="icon wb-alert-circle text-no-answer" style="font-size:24px;" aria-hidden="true"></i>
                      <span class="badge badge-pill up badge-info" id="event-count-no-answer"></span>
                    </span>
                  </a>
                </li>
              </ul>
              <div class="tab-content">
                <div class="tab-pane active show" id="exampleTabsIconOne" role="tabpanel">
                  <div class="table-responsive">
                    <table class="table">
                      <tbody id="table-resp-ok">
                        <td class="text-center">NO USER</td>
                      </tbody>
                    </table>
                  </div>
                </div>
                <div class="tab-pane" id="exampleTabsIconTwo" role="tabpanel">
                  <div class="table-responsive">
                    <table class="table">
                      <tbody id="table-resp-no">
                        <td class="text-center">NO USER</td>
                      </tbody>
                    </table>
                  </div>
                </div>
                <div class="tab-pane" id="exampleTabsIconThree" role="tabpanel">
                  <div class="table-responsive">
                    <table class="table">
                      <tbody id="table-resp-uncertain">
                        <td class="text-center">NO USER</td>
                      </tbody>
                    </table>
                  </div>
                </div>
                <div class="tab-pane" id="exampleTabsIconFour" role="tabpanel">
                  <div class="table-responsive">
                    <table class="table">
                      <tbody id="table-resp-no-answer">
                        <td class="text-center">NO USER</td>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


